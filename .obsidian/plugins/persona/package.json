{
  "name": "obsidian-persona",
  "version": "0.1.0",
  "description": "Run Persona AI agents from Obsidian",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ui": "jest --testPathPattern=ui",
    "test:services": "jest --testPathPattern=services",
    "test:all": "../../../Projects/Persona/scripts/test-all.sh",
    "test:all:save": "../../../Projects/Persona/scripts/test-all.sh --save",
    "test:manifest": "../../../Projects/Persona/scripts/validate-manifest.sh"
  },
  "keywords": [
    "obsidian",
    "persona",
    "ai",
    "agents"
  ],
  "author": "pbarrick",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "@supabase/supabase-js": "^2.93.3"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.0.0",
    "builtin-modules": "^3.3.0",
    "esbuild": "^0.20.0",
    "jest": "^29.7.0",
    "jest-environment-obsidian": "^0.0.1",
    "obsidian": "latest",
    "ts-jest": "^29.1.2",
    "typescript": "^5.0.0"
  }
}
